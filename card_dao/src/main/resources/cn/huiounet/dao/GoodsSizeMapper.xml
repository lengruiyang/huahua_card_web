<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.huiounet.dao.GoodsSizeMapper">

    <select id="findGoodsSize" resultType="GoodsSize">
        SELECT * FROM  card_goods_size where color_id = #{color_id} AND status_ = '1';
    </select>

    <select id="findById" resultType="GoodsSize">
        SELECT * FROM  card_goods_size where id = #{id};
    </select>

</mapper>