<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.huiounet.dao.MiaoShaGoodsSysMapper">

    <update id="updateStartTime">
        UPDATE card_miao_sha SET start_time = #{start_time} WHERE id = #{id}
    </update>

    <select id="findById" resultType="MiaoShaGoodsSys">
        SELECT * FROM card_miao_sha WHERE id = #{id};
    </select>


</mapper>