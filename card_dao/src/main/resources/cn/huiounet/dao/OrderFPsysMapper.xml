<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.huiounet.dao.OrderFPsysMapper">

    <select id="findByOrderNum" resultType="OrderFPsys">
        SELECT * FROM  order_fp_sys where order_num = #{order_num};
    </select>

    <select id="findByUserId" resultType="OrderFPsys">
        SELECT * FROM  order_fp_sys where user_id = #{user_id};
    </select>

</mapper>