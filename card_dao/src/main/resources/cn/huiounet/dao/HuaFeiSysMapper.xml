<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.huiounet.dao.HuaFeiSysMapper">

    <select id="findAll" resultType="HuaFeiSys">
    SELECT * FROM  card_huafei_sys where status =  '1';
</select>

    <select id="findById" resultType="HuaFeiSys">
        SELECT * FROM  card_huafei_sys where id =  #{id};
    </select>


</mapper>